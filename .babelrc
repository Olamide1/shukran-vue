{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8", "Chrome > 41"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-runtime", ["transform-object-rest-spread", { "useBuiltIns": true }]],
  "env": {
    "test": {
      "presets": ["@babel/preset-env"],
      "plugins": [ "istanbul" ]
    }
  }
}
